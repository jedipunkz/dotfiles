snippet opsgenie
	provider "opsgenie" {
		api_key = "${1}"
	}
snippet r_opsgenie_team
	resource "opsgenie_team" "${1}" {
		name = "${2}"
	}
snippet r_opsgenie_user
	resource "opsgenie_user" "${1}" {
		full_name = "${2}"
		role = "${3}"
		username = "${4}"
	}
snippet d_opsgenie_user
	data "opsgenie_user" "${1}" {
		username = "${2}"
	}
