		name = "${2}"
	}
snippet r_cloudstack_security_group_rule
	resource "cloudstack_security_group_rule" "${1}" {
		rule {
			protocol = ""
		}
		security_group_id = "${3}"
	}
snippet r_cloudstack_ssh_keypair
	resource "cloudstack_ssh_keypair" "${1}" {
		name = "${2}"
	}
snippet r_cloudstack_static_nat
