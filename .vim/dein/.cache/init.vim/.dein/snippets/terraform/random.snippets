snippet random
	provider "random" {
		
	}
snippet r_random_id
	resource "random_id" "${1}" {
		byte_length = ${2}
	}
snippet r_random_integer
	resource "random_integer" "${1}" {
		max = ${2}
		min = ${3}
	}
snippet r_random_pet
	resource "random_pet" "${1}" {
		
	}
snippet r_random_shuffle
	resource "random_shuffle" "${1}" {
		input = ["${2}"]
	}
snippet r_random_string
	resource "random_string" "${1}" {
		length = ${2}
	}
snippet r_random_uuid
	resource "random_uuid" "${1}" {
		
	}
