add_newline = false

[character]
symbol = ">>"
error_symbol = "✗"
style_success = "#adff2f"
style_failure = "red"
use_symbol_for_status = true

[directory]
truncation_length = 3
truncate_to_repo = false
prefix = "in "
style = "#87cefa"

[time]
disabled = false
format = "[ %T ]"
utc_time_offset = "+9"
style = "#1e90ff"

[aws]
style = "bold blue"
symbol = "🅰 "
displayed_items = "region"
# [aws.region_aliases]
# ap-southeast-2 = "au"
# us-east-1 = "va"

[docker_context]
symbol = "🐋 "

[git_branch]
symbol = "🌱 "
#truncation_length = 2
#truncation_symbol = ""

[git_state]
progress_divider = " of "
cherry_pick = "🍒 PICKING"

[git_status]
conflicted = "🏳"
ahead = "🏎💨"
behind = "😰"
diverged = "😵"
untracked = "🤷‍"
stashed = "📦"
modified = "📝"
staged.value = "++"
taged.style = "green"
staged_count.enabled = true
staged_count.style = "green"
renamed = "👅"
deleted = "🗑"

[golang]
#symbol = "🏎💨 "

[kubernetes]
symbol = "⛵ "
style = "dimmed green"
disabled = false
[kubernetes.context_aliases]
"dev.local.cluster.k8s" = "dev"

[nodejs]
symbol = "🤖 "

[python]
# symbol = "👾 "
pyenv_version_name = true
pyenv_prefix = "foo "

[ruby]
symbol = "🔺 "

[terraform]
symbol = "🏎💨 "
show_version = true



