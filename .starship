add_newline = false
scan_timeout = 10
format = """\
    $username\
    $hostname\
    $directory\
    $git_branch\
    $git_commit\
    $git_state\
    $git_status\
    $hg_branch\
    $env_var\
    $docker_context\
    $kubernetes\
    $aws\
    $package\
    $dotnet\
    $elixir\
    $elm\
    $erlang\
    $golang\
    $java\
    $julia\
    $nodejs\
    $ocaml\
    $php\
    $purescript\
    $python\
    $ruby\
    $rust\
    $terraform\
    $zig\
    $nix_shell\
    $conda\
    $memory_usage\
    $crystal\
    $cmd_duration\
    $custom\
    $line_break\
    $jobs\
    $battery\
    $time\
    $character\
"""

[character]
success_symbol = "[ ::](#eeef20)"
error_symbol = "[ ERROR](#ff5859)"
vicmd_symbol = "[ ::](#55FF55)"

[hostname]
ssh_only = true
disabled = false
style = "#ffff3f"

[directory]
truncation_length = 3
truncate_to_repo = false
style = "#e4ff1a"

[time]
disabled = false
format = "[$time]($style)"
time_format = "%T"
utc_time_offset = "+9"
style = "#adb5bd"

[aws]
symbol = "â˜ï¸  "
style = "#ff9900"
