add_newline = false
scan_timeout = 10
prompt_order = [
    "username",
    "hostname",
    "directory",
    "git_branch",
    "git_commit",
    "git_state",
    "git_status",
    "hg_branch",
    "aws",
    "env_var",
    "docker_context",
    "kubernetes",
    "package",
    "dotnet",
    "elixir",
    "elm",
    "erlang",
    "golang",
    "haskell",
    "java",
    "julia",
    "nodejs",
    "ocaml",
    "php",
    "purescript",
    "python",
    "ruby",
    "rust",
    "terraform",
    "zig",
    "nix_shell",
    "conda",
    "memory_usage",
    "crystal",
    "cmd_duration",
    "custom",
    "line_break",
    "jobs",
    "battery",
    "time",
    "character",
]

[character]
symbol = "::"
error_symbol = "💥"
style_success = "#dae1e7"
style_failure = "red"
use_symbol_for_status = true

[directory]
truncation_length = 3
truncate_to_repo = false
prefix = "in "
style = "#2fc4b2"

[time]
disabled = false
format = "[ %T ]"
utc_time_offset = "+9"
style = "#40bad5"

[aws]
#style = "orange"
symbol = "☁️  "
displayed_items = "all"
# [aws.region_aliases]
# ap-southeast-2 = "au"
# us-east-1 = "va"

[docker_context]
symbol = "🐋 "

[git_branch]
symbol = "🌵 "
#truncation_length = 2
#truncation_symbol = ""
style = "#f96d80"

[git_state]
progress_divider = " of "
cherry_pick = "🍒 PICKING"

[git_status]
#conflicted = "🏳"
#ahead = "🏎💨"
#behind = "😰"
#diverged = "😵"
#untracked = "🤷‍"
#stashed = "📦"
modified = "🌸"
staged.value = "++"
taged.style = "green"
staged_count.enabled = true
staged_count.style = "green"
renamed = "👅"
deleted = "🌼"

[golang]
#symbol = "🏎💨 "

[kubernetes]
symbol = "🥑 "
style = "#ffd31d"
disabled = false
[kubernetes.context_aliases]
"dev.local.cluster.k8s" = "dev"

[env_var]
variable = "AWSPROFILE"
#default = "None"
style = "#FFFFFF"
prefix = "☁️  "
suffix = ""

[nodejs]
symbol = "🤖 "

[python]
# symbol = "👾 "
pyenv_version_name = true
pyenv_prefix = "foo "

[ruby]
symbol = "🔺 "

[terraform]
#symbol = "🏎💨 "
#show_version = true

[battery]
disabled = true

[conda]
disabled = true

[crystal]
disabled = true

[dotnet]
disabled = true

[elixir]
disabled = true

[elm]
disabled = true

[erlang]
disabled = true

